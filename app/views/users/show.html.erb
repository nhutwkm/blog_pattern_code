
<h style="color:red">list user<br></h>
<table border="1" style="width:50%">
  <thead>
    <tr>
       <th>id</th>
      <th><th>name</th></th>
      <th><th>email</th></th>
      <th><th>role_id</th></th>
      <th colspan="4"></th>
    </tr>
  </thead>

  <tbody>
    <% @user.each do |student| %>
      <tr>
        <td><%= @id=student.id %></td>
        <td><td><%= student.name %></td></td>
        <td><td><%= student.email %></td></td>
        <td><td><%= student.role_id %></td></td>

        <td><%= link_to 'eidt', "/edit?id=#{@id}" %></td>
      </tr>
    <% end %>
  </tbody>
</table>
<%= will_paginate @user %>
<br>
    
